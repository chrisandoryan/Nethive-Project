                                a.nav-link(href='/admin/config')
                                    i.nav-icon.fas.fa-cog
                                    p
                                        | Configurations
                                        //- span.right.badge.badge-danger New
                            li.nav-item.has-treeview
                                .nav-link.nav-logs
                                    i.nav-icon.fas.fa-copy
                                    p
                                        | Logs
                                        i.fas.fa-angle-left.right
                                        //- span.badge.badge-info.right 3
                                ul.nav.nav-treeview
                                    li.nav-item
                                        a.nav-link(href='/admin/logs/web')
                                            i.far.fa-circle.nav-icon
