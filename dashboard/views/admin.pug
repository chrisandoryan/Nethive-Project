                                    li.nav-item
                                        a.nav-link(href='/admin/logs/audit')
                                            i.far.fa-circle.nav-icon
                                            p Audit(syscall)
                                    li.nav-item
                                        a.nav-link(href='/admin/logs/bash')
                                            i.far.fa-circle.nav-icon
                                            p Bash History
                                li.nav-header MISCELLANEOUS
                                    li.nav-item
                                        a.nav-link(href='https://adminlte.io/docs/3.0')
                                            i.nav-icon.fas.fa-file
                                            p Documentation
            .content-wrapper
                //- include pages/logs/logsBashPage
                include pages/dashboardPage

