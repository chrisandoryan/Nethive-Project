                        li.nav-item
                            a.nav-link(data-widget='control-sidebar' data-slide='true' href='#')
                                i.fas.fa-th-large

            aside.main-sidebar.sidebar-dark-primary.elevation-4
                // Brand Logo
                a.brand-link(href='index3.html')
                    img.brand-image.img-circle.elevation-3(src='/images/just-logo.svg' alt='Logo' style='opacity: .8')
                    span.brand-text.font-weight-light NetHive
                // Sidebar
                .sidebar
                    // Sidebar user panel (optional)
                    .user-panel.mt-3.pb-3.mb-3.d-flex
                        .image
                            img.img-circle.elevation-2(src='/images/face-placeholder.png' alt='User Image')
                        .info
                            a.d-block(href='#') System Admin
                    // Sidebar Menu
                    nav.mt-2
                        ul.nav.nav-pills.nav-sidebar.flex-column.nav-child-indent(data-widget='treeview' role='menu' data-accordion='false')
                            //
                            //Add icons to the links using the .nav-icon class
                            //with font-awesome or any other icon font library
                            li.nav-item.has-treeview
                                a.nav-link.active(href='/admin/dashboard')
